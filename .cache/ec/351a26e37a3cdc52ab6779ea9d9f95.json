lue()];\r\n    myChart.setOption({\r\n      xAxis: {\r\n        data: xData\r\n      },\r\n      series: [{data: seriesData}]\r\n    });\r\n    myChart.hideLoading();\r\n    isLoading = false;\r\n  }, 3000);\r\n});\r\nmyChart.on('click', (e) => {\r\n  console.log(e);\r\n  console.log(e.dataIndex);\r\n  console.log(e.data);\r\n  window.open(`https://www.baidu.com/?time=${e.dataIndex}`);\r\n\r\n});\r\n\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"4bcdd2552a0cad3a48f42de8ddb31829","cacheData":{"env":{}}}